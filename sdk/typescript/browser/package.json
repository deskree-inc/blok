{
	"name": "@blok-ts/sdk-browser",
	"version": "0.1.0",
	"description": "Official TypeScript SDK for Blok Framework - Browser runtime with React/Vue integrations",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"module": "dist/index.mjs",
	"unpkg": "dist/blok-browser-sdk.umd.js",
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"require": "./dist/index.js",
			"types": "./dist/index.d.ts"
		},
		"./react": {
			"import": "./dist/react.mjs",
			"require": "./dist/react.js",
			"types": "./dist/react.d.ts"
		},
		"./vue": {
			"import": "./dist/vue.mjs",
			"require": "./dist/vue.js",
			"types": "./dist/vue.d.ts"
		}
	},
	"files": ["dist/"],
	"scripts": {
		"build": "tsc && tsc -p tsconfig.esm.json",
		"test": "vitest",
		"test:coverage": "vitest --coverage",
		"dev": "tsc --watch",
		"prepublishOnly": "npm run build"
	},
	"keywords": [
		"blok",
		"workflow",
		"sdk",
		"browser",
		"typescript",
		"react",
		"vue",
		"frontend"
	],
	"author": "Blok Framework",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/deskree-inc/blok.git",
		"directory": "sdk/typescript/browser"
	},
	"dependencies": {},
	"devDependencies": {
		"@types/react": "^18.0.0",
		"typescript": "^5.0.0",
		"vite": "^5.0.0",
		"vitest": "^1.0.0",
		"@vitest/coverage-v8": "^1.0.0",
		"vue": "^3.3.0"
	},
	"peerDependencies": {
		"react": ">=16.8.0",
		"vue": ">=3.0.0"
	},
	"peerDependenciesMeta": {
		"react": {
			"optional": true
		},
		"vue": {
			"optional": true
		}
	},
	"engines": {
		"node": ">=16.0.0"
	}
}
