{
	"name": "nanoctl",
	"version": "0.0.18",
	"author": "Deskree Technologies Inc.",
	"license": "Apache-2.0",
	"description": "cli for nanoservice-ts",
	"engines": {
		"node": ">=18.0.0"
	},
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"type": "module",
	"bin": {
		"nanoctl": "./dist/index.js"
	},
	"files": ["dist", "node_modules"],
	"scripts": {
		"build": "rm -rf dist && npx tsc",
		"build:dev": "tsc --watch",
		"test:dev": "vitest",
		"test": "vitest run",
		"typecheck": "tsc --noEmit --incremental",
		"clean": "rm -rf dist && rm -rf node_modules",
		"install:local": "npm link",
		"uninstall:local": "npm rm --global nanoctl",
		"dev": "npm run uninstall:local && npm run install:local && npm run build:dev && nanoctl --help"
	},
	"keywords": ["nanoctl", "cli", "nanoservice", "nanoservice-ts"],
	"dependencies": {
		"@clack/prompts": "^0.9.1",
		"commander": "^13.1.0",
		"figlet": "^1.8.0",
		"fs-extra": "^11.2.0",
		"picocolors": "^1.1.1",
		"posthog-node": "^4.14.0",
		"simple-git": "^3.27.0",
		"type-fest": "^4.34.1",
		"typescript": "^5.5.4",
		"yocto-spinner": "^0.1.1",
		"zod": "^3.24.0"
	},
	"devDependencies": {
		"@types/figlet": "^1.7.0",
		"@types/fs-extra": "^11.0.4",
		"@types/node": "^22.13.4",
		"nodemon": "^3.1.9",
		"rimraf": "^6.0.1",
		"ts-node": "^10.9.2",
		"typescript": "^5.7.2",
		"vitest": "^3.0.4"
	},
	"private": false
}
